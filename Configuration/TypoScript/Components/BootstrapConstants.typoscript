@import 'EXT:bootstrap_package/Configuration/TypoScript/constants.typoscript'

page {
    preloader.enable = 0
    logo {
        file = EXT:portal/Resources/Public/Icons/Extension.svg
        fileInverted =
        width = 48
        height = 48
    }

    theme {
        meta {
            includeNotInMenu = 0
            navigationType = directory
            navigationValue = 6
        }

        language.enable = 0
        socialmedia.enable = 0
        copyright.enable = 0
        breadcrumb.enableLevel = 1
    }
}
